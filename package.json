{
  "name": "nested-routes",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "aws-amplify": "^1.0.10",
    "aws-s3": "^1.5.5",
    "react": "^16.5.0",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "react-toastify": "^4.3.1",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "REACT_APP_STAGE=dev react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "REACT_APP_STAGE=dev npm run build",
    "deploy": "aws s3 sync build/  s3://notes-app-client-2018",
    "postdeploy": "aws cloudfront create-invalidation --distribution-id EBN4SZ43ACFE3 --paths '/*'",
    "predeploy:prod": "REACT_APP_STAGE=production npm run build",
    "deploy:prod": "aws s3 sync build/  s3://notes-app-client-2018",
    "postdeploy:prod": "aws cloudfront create-invalidation --distribution-id EBN4SZ43ACFE3 --paths '/*'"
  }
}
